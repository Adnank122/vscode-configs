{
  "explorer.compactFolders": false,
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "name": "[ITALICS] REMOVED",
        "scope": [
          "modifier",
          "this",
          "comment",
          "storage.modifier.js",
          "entity.other.attribute-name.js",
          "entity.other.attribute-name.html",
          "punctuation.definition.comment",
          "text.html.basic entity.other",
          "entity.other.attribute-name.tsx",
          "entity.other.attribute-name.jsx",
          "markup.quote.markdown meta.paragraph.markdown",
          "markup.italic.markdown",
          "text.jade entity.other.attribute-name.tag",
          "keyword.control.from.js",
          "entity.other.attribute-name.tag.pug"
        ],
        "settings": {
          "fontStyle": ""
        }
      }
    ]
  },
  "workbench.iconTheme": "material-icon-theme",
  "editor.fontFamily": "Fira Code, Monaco, 'Courier New', monospace",
  "terminal.integrated.fontFamily": "'Fira Code', 'Inconsolata for Powerline', monospace",
  "editor.fontSize": 16,
  "editor.lineHeight": 24,
  "editor.letterSpacing": 0.5,
  "editor.fontWeight": "400",
  "files.insertFinalNewline": false,
  "editor.quickSuggestionsDelay": 0,
  "editor.fontLigatures": true,
  "editor.cursorStyle": "line",
  "editor.cursorWidth": 5,
  "editor.cursorBlinking": "solid",
  "editor.renderWhitespace": "all",
  "editor.snippetSuggestions": "top",
  "editor.glyphMargin": true,
  "workbench.editor.enablePreview": false,
  "explorer.confirmDragAndDrop": false,
  "files.trimTrailingWhitespace": true,
  "files.trimFinalNewlines": true,
  "explorer.openEditors.visible": 2,
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "eslint.run": "onType",
  "eslint.enable": true,
  "terminal.integrated.macOptionIsMeta": true,
  "highlight-matching-tag.styles": {
    "opening": {
      "full": {
        "custom": {
          "backgroundColor": "rgba(165, 153, 233, 0.1)"
        }
      },
      "left": {
        "custom": {
          "borderWidth": "0"
        }
      },
      "right": {
        "custom": {
          "borderWidth": "0"
        }
      }
    }
  },
  "[markdown]": {
    "editor.wordWrap": "on",
    "editor.quickSuggestions": true,
    "editor.formatOnSave": true
  },
  "editor.quickSuggestions": {
    "strings": true
  },
  "importCost.largePackageColor": "#EC3A37F5",
  "importCost.mediumPackageColor": "#B362FF",
  "importCost.smallPackageColor": "#B362FF",
  "workbench.panel.defaultLocation": "right",
  "workbench.settings.editor": "json",
  "editor.minimap.enabled": false,
  "window.zoomLevel": 0,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.colorCustomizations": {},
  "vim.easymotion": true,
  "vim.incsearch": true,
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": true,
  "vim.hlsearch": true,
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "j"],
      "after": ["<Esc>"]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "d"],
      "after": ["d", "d"]
    },
    {
      "before": ["<C-n>"],
      "commands": [":nohl"]
    }
  ],
  "vim.leader": "<space>",
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-f>": false
  },
  "vsicons.dontShowNewVersionMessage": true,
  "explorer.confirmDelete": false,
  "workbench.colorTheme": "Cobalt2",
  "workbench.editor.showTabs": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "gitlens.mode.active": "zen",
  "diffEditor.ignoreTrimWhitespace": false
}
